{
  "name": "platformkit",
  "description": "Opinionated SaaS Framework",
  "version": "0.0.2",
  "author": "James Hunter Carter",
  "engines": {
    "node": ">=22.0.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "node scripts/preinstall.js;",
    "install-cli": "cd cli && npm i;",
    "install-ui": "node scripts/install-ui.js;",
    "install-server": "cd server; npm i;",
    "install-storybook": "cd storybook && npm i",
    "install-devcontainer": "node scripts/install-devcontainer.js",
    "install-devpod": "node scripts/install-devpod.js",
    "install-for-deploy": "npm run install-ui && npm run install-cli && npm run install-server;",
    "install-devtools": "npm run install-devpod; npm run setup-devpod; npm run install-devcontainer; npm run install-supabase;",
    "setup-devpod": "node scripts/setup-devpod.js",
    "start-devpod": "node scripts/start-devpod.js",
    "install-database": "node scripts/install-database.js",
    "uninstall-database": "node scripts/uninstall-database.js",
    "start-database": "node scripts/start-database.js",
    "stop-database": "node scripts/stop-database.js",
    "install-supabase": "npx supabase init",
    "uninstall-supabase": "node scripts/uninstall-supabase.js",
    "start-supabase": "node scripts/start-supabase.js",
    "stop-supabase": "npx supabase stop",
    "introspect-database-schema": "drizzle-kit introspect --config=./database/drizzle/database.config.js",
    "generate-database-schema": "drizzle-kit generate --config=./database/drizzle/database.config.js",
    "prisma-client-generate-serverless": "npx prisma generate --no-engine --schema=./database/prisma/schema.prisma",
    "prisma-client-generate": "prisma generate --schema=./database/prisma/schema.prisma",
    "prisma-client-generate-proxy": "prisma generate --schema=./database/prisma/schema.prisma --data-proxy",
    "prisma-schema-format": "prisma format --schema=./database/prisma/schema.prisma",
    "prisma-schema-migrate-dev": "prisma migrate dev --schema=./database/prisma/schema.prisma",
    "prisma-migrate": "prisma migrate deploy --schema=./database/prisma/schema.prisma",
    "prisma-migrate-production": "node scripts/prisma-migrate.js",
    "cli": "./cli/bin/run.js",
    "cli-dev": "cd cli; npm run build; ./bin/run.js",
    "dev": "node scripts/devcontainer.js",
    "dev-api": "node scripts/dev-api.js",
    "dev-ui": "node scripts/dev-ui.js",
    "dev-server": "npm run build-api-spec; node scripts/dev.js",
    "validate-api": "npm run build-api-spec; scalar validate api/openapi-spec.yml",
    "mock-api": "npm run build-api-spec; scalar mock api/openapi-spec.json --watch --port 8080",
    "deploy": "npm run refresh-ui; npm run build-functions; npm run build-api; npm run build-ui;",
    "build-docker": "docker compose --env-file .env up --build",
    "build-keys": "node scripts/generate-keys.js",
    "build-ui": "npm run refresh-ui; cd ui && npm i; npm run generate",
    "build-functions": "node scripts/build-functions.js;",
    "build-api": "npm run install-server; npm run build-api-spec;",
    "build-api-spec": "node api/generate-api-schema.js;",
    "build-styles": "cd styles && node scripts/generate.mjs",
    "refresh": "npm run refresh-ui; npm run refresh-node-modules;",
    "refresh-node-modules": "cd ui; rm -rf node_modules; npm i;",
    "refresh-ui": "cd ui; rm -rf dist; rm -rf .nuxt; rm -rf .output;",
    "serve": "nodemon scripts/serve.js --watch functions --watch .workspace/functions;",
    "serve-proxy": "node server/proxy.js",
    "sdk-generate": "kubb generate --config ./sdk/sdk.config.mjs",
    "test-api": "node scripts/test-api.js",
    "start-browser": "node scripts/start-browser.js",
    "start-devcontainer": "node scripts/start-devcontainer.js",
    "clone-workspace": "node scripts/clone-workspace.js",
    "push-workspace": "node scripts/push-workspace.js"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@kubb/cli": "^2.19.6",
    "@kubb/core": "^2.19.6",
    "@kubb/plugin-oas": "^2.19.6",
    "@kubb/swagger-client": "^2.19.6",
    "@kubb/swagger-ts": "^2.19.6",
    "@prisma/client": "^5.16.2",
    "@prisma/extension-accelerate": "^1.1.0",
    "@scalar/api-reference": "^1.24.10",
    "@scalar/cli": "^0.2.115",
    "@sentry/node": "^8.9.2",
    "@usebruno/cli": "^1.20.0",
    "async-wait-until": "^2.0.12",
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "config": "^3.3.12",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "http": "^0.0.1-security",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "open": "^10.1.0",
    "postgres": "^3.4.4",
    "prisma": "^5.16.2",
    "selfsigned": "^2.4.1",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@devcontainers/cli": "^0.65.0",
    "drizzle-kit": "^0.22.7"
  }
}
